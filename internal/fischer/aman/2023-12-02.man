#!perl

use strict;
use integer;
my $state = 0;
my ($line, $aseqno, $inits, $form, $offset);
while(<DATA>) {
    s/\s+\Z//; # chompr;
    $line = substr($_, 3);
    $aseqno = substr($line, 0, 7);
    $line = substr($line, 8);
    if (0) {
    } elsif ($state == 0) {
        if ($line =~ m{remember}) {
            $line =~ m{\[([^\]]+)};
            $inits = $1;
            $inits =~ s{ }{}g;
            $state = 1;
        }
    } elsif ($state == 1) {
        $line =~ s{ }{}g;
        if ($line =~ s{end.*}{}) {
            $form .= $line;
            $state = 2;
        } else {
            $form .= $line;
        }
    } elsif ($state == 2) {
        if ($line =~ m{seq\(a\(n\)\, *n\ *\= *(\d+) *\.\.}) {
            $offset = $1;
            if ($form =~ s{\/(.*)}{}) {
                my $num = $1;
                $num =~ s{\)\Z}{};
                $form = "-a(n)*$num+$form=0";
            } else {
                $form = "-a(n)+$form=0";
            }
            print join("\t", $aseqno, "bva", $offset, $form, $inits, 0, 0) ."\n";
            $form = "";
            $state = 0;
        }
    }
} # while <DATA>
